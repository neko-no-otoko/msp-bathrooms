# SMTP Configuration for Suggestion Emails
# Copy this file to .env and fill in the values

SMTP_HOST=smtp.example.com
SMTP_PORT=587
# Set to 'true' if using port 465, 'false' for 587 or 25
SMTP_SECURE=false

# SMTP Config
SMTP_USER=your-email@example.com
SMTP_PASS=your-email-password

# Email Routing
SMTP_FROM=no-reply@mn-bathroom-tracker.com
SMTP_TO=admin@mn-bathroom-tracker.com
